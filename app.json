{
  "expo": {
    "name": "Nhabaola",
    "description": "Nhabaola",
    "slug": "nhabaola-reactnative",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.8",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "notification": {
      "icon": "./assets/icons/app-icon.png",
      "color": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": true,
      "backgroundColor": "#a4d227"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "vn.nhabaola.nhabaola",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription ":"Your current location will be displayed on the map and used for directions, nearby search result and estimated distance.",
        "NSLocationWhenInUseUsageDescription": "Your current location will be displayed on the map and used for directions, nearby search result and estimated distance"
      }
    },
    "android": {
      "package": "vn.nhabaola.nhabaola",
      "versionCode": 8,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDjGSoHkMu6Q55FifG90l25DR8SfMo3quM"
        }
      }
    }
  }
}